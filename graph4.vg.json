{
"$schema": "https://vega.github.io/schema/vega-lite/v5.json",
"title": { "text": "genre most likely to chart" },
"data": {
"url": "spotify.csv"
},
 "encoding": {
    "theta": {"aggregate": "count", "field": "genre", "type": "quantitative", "stack": true},
    "color": {"field": "genre", "type": "nominal", "legend": null}
  },
  "layer": [{
    "mark": {"type": "arc", "outerRadius": 80}
  }, {
    "mark": {"type": "text", "radius": 90},
    "encoding": {
      "text": {"field": "genre", "type": "nominal"}
    }
  }]
}





