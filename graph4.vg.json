{
"$schema": "https://vega.github.io/schema/vega-lite/v5.json",
"title": { "text": "genre most likely to chart -> items <3 omitted" },
"width": 600,
"height": 600,
"data": {
"url": "spotify_genre.csv"
},
 "encoding": {
    "theta": {"field": "count", "type": "quantitative", "stack": true},
    "color": {"field": "Genre", "type": "nominal", "legend": null}
  },
  "layer": [{
    "mark": {"type": "arc", "outerRadius": 150}
  }, {
    "mark": {"type": "text", "radius": 250},
    "encoding": {
      "text": {"field": "Genre", "color": "black", "type": "nominal"}
    }
  }]
}





